import Vue from 'vue'
import VueRouter from "vue-router";
Vue.use(VueRouter)
export default new VueRouter({
  mode: 'hash',
  routes: [
    {
      path: '/Home',
      component: () => import(/* webpackChunkName: "Home" */ '../views/Home.vue')
    },
    {{#each list}}
    {
      path: '/{{name}}',
      component: () => import(/* webpackChunkName: "{{name}}" */ '../views/{{file}}')
    },
    {{/each}}
    {
      path: '*',
      redirect: '/Home'
    }
  ]
})